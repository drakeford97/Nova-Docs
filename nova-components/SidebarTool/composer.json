{
    "name": "otwell/sidebar-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Otwell\\SidebarTool\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./../../nova"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Otwell\\SidebarTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
